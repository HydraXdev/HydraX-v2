        session_gen_prob = {
            'OVERLAP': 0.04,    # 4% chance - ~2 signals per hour max
            'LONDON': 0.03,     # 3% chance
            'NY': 0.03,         # 3% chance
            'ASIAN': 0.01,      # 1% chance - very rare
            'OFF_HOURS': 0.0    # Never trade off-hours
